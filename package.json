    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "scripts": {
    "build": "cross-env-shell NODE_ENV=production \"npm run build-css && npm run build-client && npm run build-server\"",
    "build-css": "run-p -s build-css:*",
    "build-css:debug": "sass --load-path=client --source-map assets/stylesheets/styles.scss public/styles-debug.css",
    "build-css:style": "sass --load-path=client --style=compressed assets/stylesheets/styles.scss public/styles.min.css",
    "build-client": "webpack",
    "build-client:dev": "webpack --watch --progress",
    "build-server": "webpack --config webpack.config.node.js",
    "server": "npm run build-server && node build/server.js",
    "start": "cross-env-shell NODE_ENV=development \"npm run build-css && run-p -s build-client:dev server\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
